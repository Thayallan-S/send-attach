{
    "name": "SendAttach",
    "version": "0.1",
    "content_scripts": [
      {
        "matches": [
          "https://mail.google.com/*",
          "https://inbox.google.com/*"
        ],
        "js": ["inboxsdk.js","content.js"],
        "run_at": "document_start"
      }
    ],
    "web_accessible_resources": [
      "plane.png"
    ],
    "permissions": [
      "https://mail.google.com/",
      "https://inbox.google.com/"
    ],
    "manifest_version": 2
  }