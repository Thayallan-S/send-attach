{
    "manifest_version" : 2,
    "name" : "SendAttach",
    "version" : "0.1",
    "background": {
        "scripts": ["background.js"]
    },      
    "content_scripts" : [
        {
            "matches" : [
                "https://mail.google.com/*", 
                "http://mail.google.com/*"
            ],
            "js" : [
                "jquery-3.4.1.min.js",
                "content.js"
            ]
        }
    ],
    "browser_action": {
        "default_icon": "icon.png"
    }
}